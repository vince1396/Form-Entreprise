RewriteEngine On
RewriteCond %(REQUEST_FILENAME) !-d
RewriteCond %(REQUEST_FILENAME) !-f
RewriteRule ^([a-zA-Z-]+)$ http://www.allburotic.portfolio-vcotini.com/index.php?p=$1 [QSA,L]
RewriteRule ^([a-zA-Z-]+)\/([0-9]+)$ http://www.allburotic.portfolio-vcotini.com/index.php?p=$1&id=$2&id=$3 [QSA,L]

